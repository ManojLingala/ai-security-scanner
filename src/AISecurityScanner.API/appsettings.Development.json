{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Raven": "Information"
    }
  },
  "RavenDb": {
    "Urls": ["http://localhost:8080"],
    "Database": "AISecurityScanner-Dev",
    "UseEmbedded": false
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379,abortConnect=false"
  },
  "Jwt": {
    "Secret": "YourDevelopmentSecretKeyHereMustBeAtLeast256Bits12345!@#$%",
    "Issuer": "AISecurityScanner-Dev",
    "Audience": "AISecurityScanner-Dev",
    "ExpirationMinutes": 1440
  },
  "AIProviders": {
    "OpenAI": {
      "ApiKey": "your-openai-api-key-here",
      "ApiEndpoint": "https://api.openai.com/v1/chat/completions",
      "Model": "gpt-4-turbo-preview",
      "MaxTokens": 4096,
      "CostPerRequest": 0.03
    },
    "Claude": {
      "ApiKey": "your-anthropic-api-key-here",
      "ApiEndpoint": "https://api.anthropic.com/v1/messages",
      "Model": "claude-3-sonnet-20240229",
      "MaxTokens": 4096,
      "CostPerRequest": 0.015
    },
    "MLVulnerabilityDetector": {
      "Enabled": true,
      "CostPerRequest": 0.001,
      "MaxCodeLength": 100000,
      "ConfidenceThreshold": 0.3
    },
    "HybridML": {
      "Enabled": true,
      "UseLocalML": true,
      "UseExternalAPIs": false,
      "CostPerRequest": 0.05
    },
    "CodeGuru": {
      "ApiKey": "",
      "ApiEndpoint": "https://codeguru-reviewer.amazonaws.com",
      "Enabled": false
    },
    "Snyk": {
      "ApiKey": "",
      "ApiEndpoint": "https://snyk.io/api/v1",
      "Enabled": false
    }
  }
}
